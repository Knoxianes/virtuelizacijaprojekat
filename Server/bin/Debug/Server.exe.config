<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <add key="database" value="inmemory"/>
    <add key="calculation" value="apd"/>
  </appSettings>
  <system.serviceModel>
    <services>
      <service name="Server.Servis">
        <endpoint address="" binding="netTcpBinding" contract="Common.IServis" />
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:4000/IServis" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <client>
      <endpoint name="DataBase"
        address="net.tcp://localhost:4001/DataBase"
        binding="netTcpBinding"
        contract="Common.IDataBase" />
    </client>
  </system.serviceModel>
</configuration>